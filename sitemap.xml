---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
  xmlns:image="http://www.google.com/schemas/sitemap-image/1.1">
  <url>
    <loc>{{ site.production_url }}</loc>
    <image:image>
       <image:loc>{{ site.production_url }}/static/images/devblog.jpg</image:loc>
    </image:image>
  </url>
  <url><loc>{{ site.production_url }}/archives.html</loc></url>
  {% for post in site.posts %}
  <url><loc>{{ site.production_url }}{{ post.id }}/</loc></url>
  {% endfor %}
</urlset>
